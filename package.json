{
  "name": "x402-sponsor-relay",
  "version": "1.10.1",
  "private": true,
  "scripts": {
    "dev": "npm run wrangler -- dev",
    "deploy:staging": "npm run wrangler -- deploy --env staging",
    "deploy:production": "npm run wrangler -- deploy --env production",
    "deploy:dry-run": "npm run wrangler -- deploy --dry-run",
    "check": "tsc --noEmit",
    "wrangler": "set -a && . ./.env && set +a && npx wrangler",
    "cf-typegen": "npm run wrangler -- types",
    "test:relay": "set -a && . ./.env && set +a && tsx scripts/test-relay.ts",
    "test:sponsor": "set -a && . ./.env && set +a && tsx scripts/test-sponsor.ts",
    "test:provision": "set -a && . ./.env && set +a && tsx scripts/test-provision.ts",
    "test:provision-stx": "set -a && . ./.env && set +a && tsx scripts/test-provision-stx.ts",
    "test:sip018-auth": "set -a && . ./.env && set +a && tsx scripts/test-sip018-auth.ts",
    "test:settle": "set -a && . ./.env && set +a && tsx scripts/test-settle.ts",
    "test:fees": "set -a && . ./.env && set +a && tsx scripts/test-fees.ts",
    "keys": "set -a && . ./.env && set +a && tsx scripts/manage-api-keys.ts"
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "wrangler": "^4.58.0"
  },
  "dependencies": {
    "@stacks/common": "^7.3.1",
    "@stacks/encryption": "^7.3.1",
    "@stacks/transactions": "^7.3.1",
    "@stacks/wallet-sdk": "^7.2.0",
    "bip32": "^5.0.0",
    "bip39": "^3.1.0",
    "bitcoinjs-lib": "^7.0.1",
    "bitcoinjs-message": "^2.2.0",
    "chanfana": "^3.0.0",
    "ecpair": "^3.0.1",
    "hono": "^4.11.4",
    "tiny-secp256k1": "^2.2.4",
    "x402-stacks": "github:whoabuddy/x402Stacks#feat/sponsored-transactions"
  }
}
